#include<bits/stdc++.h>

using namespace std;

int main()
{
    ios::sync_with_stdio( 0 );
    cin.tie( 0 );
    string x;
    cin >> x;
    string sol = "";
    
    for( int i = 0; i < x.size(); i ++ )
    {
        if(x[i] == 'a' || x[i] == 'e' ||x[i] == 'i' || x[i] == 'o'||x[i] == 'u'||x[i] == 'y')continue;
        if(x[i] == 'A' || x[i] == 'E' ||x[i] == 'I' || x[i] == 'O'||x[i] == 'U'||x[i] == 'Y')continue;
        
        sol+='.';
        sol += tolower(x[i]);
    }
    
    cout << sol << endl;
//    WAIT
    return 0;
}
